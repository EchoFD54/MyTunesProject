<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="200.0" prefWidth="390.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="DeletePlaylistController">
    <children>
        <Label layoutX="77.0" layoutY="76.0" text="Are you sure you want to delete this playlist?" />
        <Button fx:id="btnCancel" layoutX="118.0" layoutY="122.0" mnemonicParsing="false" onAction="#cancelAction" text="Cancel" />
        <Button fx:id="btnConfirmDelete" layoutX="210.0" layoutY="122.0" mnemonicParsing="false" onAction="#confirmDelete" text="Confirm" />
    </children>



</AnchorPane>